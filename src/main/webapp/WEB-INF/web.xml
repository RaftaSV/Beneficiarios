<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  

  <servlet>
  	<servlet-name>controllerPersona</servlet-name>
  	<display-name>controllerPersona</display-name>
  	<description></description>
  	<servlet-class>com.Rafa.Controller.controllerPersona</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>controllerCerrar</servlet-name>
  	<display-name>controllerCerrar</display-name>
  	<description></description>
  	<servlet-class>com.Rafa.Controller.controllerCerrar</servlet-class>
  </servlet>
  
 
  <servlet-mapping>
  	<servlet-name>controllerPersona</servlet-name>
  	<url-pattern>/controllerPersona</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>controllerCerrar</servlet-name>
  	<url-pattern>/controllerCerrar</url-pattern>
  </servlet-mapping>
  
</web-app>
